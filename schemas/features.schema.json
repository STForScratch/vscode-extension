{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "version": {
        "description": "The version of the feature.",
        "type": "integer"
      },
      "dateAdded": {
        "description": "The date when the feature was added.",
        "type": "string"
      },
      "id": {
        "description": "The unique identifier for the feature.",
        "type": "string"
      }
    },
    "required": ["id"]
  }
}
